class A11
{
public static void matrixmult(int x[][],int y[][])
{
int i=0,j=0,k=0;
int s[][]=new int[2][2];
for(i=0;i<x.length;i++)
{
for(j=0;j<x[i].length;j++)
{
s[i][j]=0;
for(k=0;k<x.length;k++)
{
s[i][j]+=x[i][k]*y[k][j];
}
}
}
for(i=0;i<x.length;i++)
{
for(j=0;j<x[i].length;j++)
{
System.out.print(s[i][j]);
System.out.print(" ");
}
System.out.println(" ");
}
}
public static void main(String args[])
{
matrixmult(new int[][]{{1,2},{3,4}},new int[][]{{0,1},{2,3}});
}
}
