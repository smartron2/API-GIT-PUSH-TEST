class A6
{
public static void sort1(int x[][])
{
int i=0,j=0,k=0,l=0,temp;
for(i=0;i<x.length;i++)
{
for(j=0;j<x[i].length;j++)
{
for(k=i+1;k<x.length;k++)
{
for(l=0;l<x[k].length;l++)
{
if(x[i][j]>x[k][l])
{
temp=x[i][j];
x[i][j]=x[k][l];
x[k][l]=temp;
}
}
}
}
}
for(i=0;i<x.length;i++)
{
for(j=0;j<x[i].length;j++)
{
System.out.println(x[i][j]);
}
}
}
public static void main(String args[])
{
sort1(new int[][]{{2,1,3},{4,0,8}});
}
}
