class A14
{
public static void triangleAsum(int x[][])
{
int i=0,j=0,s1=0,s2=0;
for(i=0;i<x.length;i++)
{
for(j=0;j<x[i].length;j++)
{
if(i<=j)
s1+=x[i][j];
else
s2+=x[i][j];
}
}
System.out.println("sum of upper triangular matrix is"+s1);
System.out.println("sum of lower triangular matrix is"+s2);
}
public static void main(String args[])
{
triangleAsum(new int[][]{{1,2,3},{4,5,6},{7,8,9}});
}
}
