class S16
{
public static void sortedOrder(String s)
{
int i,j;
char ch;
String w="",temp="";
String s1[];
for(i=0;i<s.length();i++)
{
ch=s.charAt(i);
if(ch!=32)
w+=ch;
}
for(i=0;i<s1.length;i++)
{
for(j=i;j<s1.length;j++)
{
if(s1[i]>s1[j])
{
temp=s1[i];
s1[i]=s1[j];
s1[j]=temp;
}
}
}
for(i=0;i<s1.length;i++)
System.out.println(s1[i]);
}
public static void main(String args[])
{
sortedOrder("i am a girl");
}
}
