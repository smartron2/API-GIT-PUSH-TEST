class S14
{
public static String changeCase(String s)
{
int i;
String w="";
for(i=0;i<s.length();i++)
{
char ch=s.charAt(i);
if(ch>='A'&&ch<='Z')
{
int d=ch;
d+=32;
ch=(char)d;
}
else if(ch>='a'&&ch<='z')
{
int d=ch;
d-=32;
ch=(char)d;
}
w+=ch;
}
return w;
}
public static void main(String args[])
{
String r=changeCase("How are you?");
System.out.println(r);
}
}
