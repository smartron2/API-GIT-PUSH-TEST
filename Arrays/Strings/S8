class S10
{
public static int vowelCount(String s)
{
int i,c=0;
for(i=0;i<s.length();i++)
{
char ch=s.charAt(i);
if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
c++;
}
return c;
}
public static void main(String args[])
{
int r=vowelCount("how are you?");
System.out.println(r);
}
}
