class A13
{
public static void matrixsum(int x[][])
{
int i=0,j=0,r,c;
for(i=0;i<x.length;i++)
{
r=0;
c=0;
for(j=0;j<x[i].length;j++)
{
r+=x[i][j];
c+=x[j][i];
}
System.out.println(r);
System.out.println(c);
}
}
public static void main(String args[])
{
matrixsum(new int[][]{{1,2,3},{4,5,6},{7,8,9}});
}
}
